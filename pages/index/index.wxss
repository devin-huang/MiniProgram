/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.components-list {
  margin-top: 50px;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.components-list > view {
  padding-top: 10px;
  padding-bottom: 10px;
}

.canvas {
  width: 130px;
  height: 130px;
}